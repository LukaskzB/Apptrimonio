<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="5dp">

    <TextView
        android:id="@+id/item_gerenciarobjetos_objetoadicionado_titulo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="125dp"
        android:fontFamily="@font/bold"
        android:text="@string/object"
        android:textColor="@color/preto"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/item_gerenciarobjetos_objetoadicionado_nome"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="125dp"
        android:fontFamily="@font/regular"
        android:text="@string/objName"
        android:textColor="@color/branco"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/item_gerenciarobjetos_objetoadicionado_titulo" />

    <LinearLayout
        android:id="@+id/item_gerneciarobjetos_objetoadicionado_linear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="5dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="@+id/item_gerenciarobjetos_objetoadicionado_nome"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/item_gerenciarobjetos_objetoadicionado_qrcode"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            android:contentDescription="@string/seeQrcode"
            android:scaleType="centerInside"
            android:src="@drawable/icon_qrcode" />

        <ImageView
            android:id="@+id/item_gerenciarobjetos_objetoadicionado_ver"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_marginStart="7dp"
            android:contentDescription="obj"
            android:scaleType="centerInside"
            android:src="@drawable/icon_view" />

    </LinearLayout>

    <ImageView
        android:id="@+id/item_gerenciarobjetos_linha"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="5dp"
        android:scaleType="centerCrop"
        android:src="@drawable/linha"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/item_gerenciarobjetos_objetoadicionado_nome" />


</androidx.constraintlayout.widget.ConstraintLayout>